{
  "manifest_version": 2,
  "name": "Collaborate.center Desktop Streamer",
  "description": "Collaborate.Center Extension will enable you to share your screen with your colleagues in a Collaborate.Center Session.",
  "version": "0.1.5",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "TabeebLogo16.png",
    "48": "TabeebLogo48.png",
    "128": "TabeebLogo128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://*.collaborate.center/*",
          "*://*.tabeeb.org/*"
      ]
  }
}
